{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "properties": {
        "setupMirrored": {
            "type": "boolean"
        },
        "contentVolumeSize": {
            "type": "string",
            "pattern": "^\\d+(?:\\.\\d+)?(?:Ki|Mi|Gi|Ti|Pi|Ei|m|k|M|G|T|P|E)?$"
        },
        "contentStorageClass": {
            "type": "string",
            "minLength": 1
        },
        "backupArchiveStorageClass": {
            "type": "string",
            "minLength": 1
        },
        "dihStorageClass": {
            "type": "string",
            "minLength": 1
        },
        "initialContentEngineCount": {
            "type": "string",
            "pattern": "^[1-9]|\\d{2,}$"
        },
        "idolImageRegistry": {
            "type": "string",
            "format": "hostname"
        },
        "idolVersion": {
            "type": "string",
            "minLength": 1
        },
        "imagePullSecrets": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "string",
                "minLength": 1
            }
        },
        "licenseServerIp": {
            "type": "string",
            "format": "ipv4"
        },
        "licenseServerPort": {
            "type": "string",
            "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
        },
        "autoscalingEnabled": {
            "type": "boolean"
        },
        "autoscalingMinReplicas": {
            "type": "string",
            "pattern": "^[1-9]|\\d{2,}$"
        },
        "autoscalingMaxReplicas": {
            "type": "string",
            "pattern": "^[1-9]|\\d{2,}$"
        },
        "autoscalingTargetAverageCpuUse": {
            "type": "string",
            "pattern": "^\\d+(?:\\.\\d+)?(?:m|k|M|G|T|P|E)?$"
        },
        "contentPrimaryBackupInterval": {
            "type": "string",
            "pattern": "^(?:[01]\\d|2[0-3]):[0-5]\\d|0$"
        },
        "contentPrimaryBackupTime": {
            "type": "string",
            "pattern": "^(?:[01]\\d|2[0-3]):[0-5]\\d$"
        },
        "ingressProxyBodySize": {
            "type": "string",
            "pattern": "^\\d+(?:\\.\\d+)?(?:Ki|Mi|Gi|Ti|Pi|Ei|m|k|M|G|T|P|E)?$"
        },
        "httpProxy": {
            "type": "string"
        },
        "licenseServerHostname": {
            "type": "string",
            "minLength": 1
        },
        "licenseServerName": {
            "type": "string",
            "minLength": 1
        },
        "livenessProbePath": {
            "type": "string",
            "minLength": 1
        },
        "queryserviceName": {
            "type": "string",
            "minLength": 1
        },
        "indexserviceName": {
            "type": "string",
            "minLength": 1
        },
        "idolingressName": {
            "type": "string",
            "minLength": 1
        },
        "prometheusingressName": {
            "type": "string",
            "minLength": 1
        },
        "autoscalingName": {
            "type": "string",
            "minLength": 1
        },
        "contentName": {
            "type": "string",
            "minLength": 1
        },
        "contentDeployment": {
            "type": "string",
            "minLength": 1
        },
        "contentImage": {
            "type": "string",
            "minLength": 1
        },
        "contentPorts": {
            "type": "array",
            "minItems": 3,
            "maxItems": 3,
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "minLength": 1
                    },
                    "service": {
                        "type": "string",
                        "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                    },
                    "container": {
                        "type": "string",
                        "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                    }
                },
                "required": [
                    "name",
                    "service",
                    "container"
                ],
                "additionalProperties": false
            }
        },
        "dahName": {
            "type": "string",
            "minLength": 1
        },
        "dahDeployment": {
            "type": "string",
            "minLength": 1
        },
        "dahImage": {
            "type": "string",
            "minLength": 1
        },
        "dahPorts": {
          "type": "array",
          "minItems": 2,
          "maxItems": 2,
          "items": {
              "type": "object",
              "properties": {
                  "name": {
                      "type": "string",
                      "minLength": 1
                  },
                  "service": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  },
                  "container": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  },
                  "host": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  }
              },
              "required": [
                  "name",
                  "service",
                  "container",
                  "host"
              ],
              "additionalProperties": false
          }
        },
        "dihName": {
            "type": "string",
            "minLength": 1
        },
        "dihDeployment": {
            "type": "string",
            "minLength": 1
        },
        "dihImage": {
            "type": "string",
            "minLength": 1
        },
        "dihPorts": {
          "type": "array",
          "minItems": 3,
          "maxItems": 3,
          "items": {
              "type": "object",
              "properties": {
                  "name": {
                      "type": "string",
                      "minLength": 1
                  },
                  "service": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  },
                  "container": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  },
                  "host": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  }
              },
              "required": [
                  "name",
                  "service",
                  "container",
                  "host"
              ],
              "additionalProperties": false
          }
        },
        "dihPrometheusPort": {
            "type": "string",
            "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
        },
        "dihPrometheusPortName": {
            "type": "string",
            "minLength": 1
        },
        "prometheusName": {
            "type": "string",
            "minLength": 1
        },
        "prometheusDeployment": {
            "type": "string",
            "minLength": 1
        },
        "prometheusService": {
            "type": "string",
            "minLength": 1
        },
        "prometheusPorts": {
          "type": "array",
          "minItems": 1,
          "maxItems": 1,
          "items": {
              "type": "object",
              "properties": {
                  "name": {
                      "type": "string",
                      "minLength": 1
                  },
                  "container": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  },
                  "host": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  }
              },
              "required": [
                  "name",
                  "container",
                  "host"
              ],
              "additionalProperties": false
          }
        },
        "custommetricsService": {
            "type": "string",
            "minLength": 1
        },
        "custommetricsPorts": {
          "type": "array",
          "minItems": 1,
          "maxItems": 1,
          "items": {
              "type": "object",
              "properties": {
                  "service": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  },
                  "host": {
                      "type": "string",
                      "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
                  }
              },
              "required": [
                  "service",
                  "host"
              ],
              "additionalProperties": false
          }
        },
        "exposedContents": {
            "type": "integer",
            "minimum": 0
        }
    },
    "required": [
        "setupMirrored",
        "contentVolumeSize",
        "backupArchiveStorageClass",
        "initialContentEngineCount",
        "idolImageRegistry",
        "idolVersion",
        "imagePullSecrets",
        "licenseServerIp",
        "licenseServerPort",
        "autoscalingMinReplicas",
        "autoscalingMaxReplicas",
        "autoscalingTargetAverageCpuUse",
        "contentPrimaryBackupInterval",
        "contentPrimaryBackupTime",
        "ingressProxyBodySize",
        "licenseServerHostname",
        "licenseServerName",
        "livenessProbePath",
        "queryserviceName",
        "indexserviceName",
        "idolingressName",
        "prometheusingressName",
        "autoscalingName",
        "contentName",
        "contentDeployment",
        "contentImage",
        "contentPorts",
        "dahName",
        "dahDeployment",
        "dahImage",
        "dahPorts",
        "dihName",
        "dihDeployment",
        "dihImage",
        "dihPorts",
        "dihPrometheusPort",
        "dihPrometheusPortName",
        "prometheusName",
        "prometheusDeployment",
        "prometheusService",
        "prometheusPorts",
        "custommetricsService",
        "custommetricsPorts",
        "exposedContents"
    ],
    "title": "Values",
    "type": "object"
}