{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "properties": {
        "licenseServerIp": {
            "type": "string",
            "format": "ipv4"
        },
        "licenseServerPort": {
            "type": "string",
            "pattern": "^((6553[0-5])|(655[0-2][0-9])|(65[0-4][0-9]{2})|(6[0-4][0-9]{3})|([1-5][0-9]{4})|([0-5]{0,5})|([0-9]{1,4}))$"
        },
        "licenseServerService": {
            "type": "string",
            "minLength": 1
        }
    },
    "required": [
        "licenseServerIp",
        "licenseServerPort",
        "licenseServerService"
    ],
    "title": "Values",
    "type": "object"
}